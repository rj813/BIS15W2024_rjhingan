badguys <- filter(superheroclean_info, alignment == "bad")
goodguys %>%
tabyl(race)
goodguys %>%
filter(race == "Vampire")
badguys %>%
select(gender, height, name) %>%
filter(gender == "Male") %>%
filter(height >= 200)
